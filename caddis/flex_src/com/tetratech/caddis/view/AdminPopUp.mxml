<?xml version="1.0" encoding="utf-8"?>
<mx:TitleWindow  title="Admin Page" xmlns:mx="http://www.adobe.com/2006/mxml" showCloseButton="true" layout="absolute" width="500" height="400">
<mx:Text paddingTop="10" paddingLeft="10" fontWeight="bold" 
	styleName="messageText" text="Diagram Configuration"/>
<mx:Form defaultButton="{saveb}" >
	<mx:FormItem>
		<mx:Spacer width="100%" height="15" />
	</mx:FormItem>
	<mx:FormItem>
		<mx:CheckBox id="publishCB" width="300" label="EPA Diagram"/> 
	</mx:FormItem>
	<!--
	<mx:FormItem>
		<mx:CheckBox id="goldSealCB" width="300" label="GOLD SEAL"/> 
	</mx:FormItem>
	-->
	
	<mx:FormItem>
		<mx:CheckBox id="lockCB" width="300" label="Lock this Diagram"/> 
	</mx:FormItem>
	
	<mx:FormItem>
		<mx:Spacer width="100%" height="15" />
	</mx:FormItem>
	
	<mx:FormItem>
		<mx:HBox horizontalAlign="center">
			<mx:Button label="Save" id="saveb">
			</mx:Button>
			<mx:Button label="Close" id="closeb">
			</mx:Button>
			<mx:Spacer width="50" />
			<mx:Button label="Review Citation" id="reviewCLink" >
			</mx:Button>
		</mx:HBox>
	</mx:FormItem>
</mx:Form>
</mx:TitleWindow>